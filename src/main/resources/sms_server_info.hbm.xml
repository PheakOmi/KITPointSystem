<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
 	<class name="com.EntityClasses.Sms_Server_Info" table="sms_server_info">
 	<id name="id" column="id" >    
 	    <generator class="increment"/>    
 	 </id>
    <property name="ip"  column="ip"/>
    <property name="db_name" column="db_name"/>
    <property name="admin_name" column="admin_name"/>
    <property name="admin_password" column="admin_password"/>
 	</class>
</hibernate-mapping>